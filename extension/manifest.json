{
  "manifest_version": 2,

  "name": "soso semantic sabotage",
  "description": "LIWC youtube remix",
  "version": "1.0",

  "permissions": ["tabs", "http://*/*", "https://*/*", "<all_urls>", "background"],
  "content_scripts": [
    {
      "matches": ["http://www.youtube.com/*"],
      "js": ["contentscript.js"],
      "css": ["style.css"]
    }
  ],
  "background": {
    "scripts": []
  },
  "web_accessible_resources": ["listen.js", "jquery-1.9.1.min.js", "localStorageDB/localstoragedb.min.js", "statsHandler.js", "parser.js", "player.js", "fill.js", "sabotage.js", "LIWC/LIWC.js"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}